@PACKAGE_INIT@

set(METIS_MAJOR_VERSION "@METIS_VER_MAJOR@")
set(METIS_MINOR_VERSION "@METIS_VER_MINOR@")
set(METIS_SUBMINOR_VERION "@METIS_VER_SUBMINOR@")

set(METIS_WITH_DEBUG "@DEBUG@")
set(METIS_WITH_OPENMP "@OPENMP@")

include(CMakeFindDependencyMacro)

# Find the GKlib dependency
find_dependency(GKlib REQUIRED)
# If necessary, find the OpenMP dependency
if(METIS_WITH_OPENMP)
  find_dependency(OpenMP REQUIRED)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/METISTargets.cmake")
check_required_components("METIS")


